source 'https://github.com/CocoaPods/Specs.git'

inhibit_all_warnings!
use_frameworks!
platform :ios, '9.0'

def shared_pods
    pod 'OneSignal'
    pod 'Mixpanel'
    pod 'Firebase'
    pod 'Firebase/Auth'
    pod 'Firebase/Database'
    pod 'R.swift'
    pod 'TableManager'
end

target 'FuelBattle' do
    shared_pods
end
